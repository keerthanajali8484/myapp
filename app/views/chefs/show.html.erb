<br><br><br>
<%= render 'shared/page_title', title: "Your Profile" %>
<div class="row">
  <div class="col-md-4 col-md-offset-4 center fixed-div">
    <%= gravatar_for @chef, size: 90 %>
  </div>
</div>
<h4 align="center"><%= @chef.name %>'s recipes</h4>
<% if @chef.recipes.count != 0 %>

<div align="center">
  
</div>
<%= render 'recipes/recipe', obj: @chef.recipes %>
<div class="center">
  <%= will_paginate @chef_recipes %>
</div>
<% else %>
  <h4 align="center">No recipes yet!</h4>
<% end %>